 {{#if url}}
    <form action="/editar/{{url._id}}" method="post">
    <input value="{{url.origin}}" type="text" name="origin" class="form-control" placeholder="origin...">
    <button type="submit" class="btn btn-warning w-100 mb-2">editar</button>
</form>

{{else}}
<form action="/" method="post">
    <input type="text" name="origin" class="form-control" placeholder="origin...">
    <button type="submit" class="btn btn-primary w-100 mb-2">add</button>
</form>


 {{/if}}


